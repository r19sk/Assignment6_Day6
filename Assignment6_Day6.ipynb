      "source": [
        "#Convert to dictionary in one line code using line comprehension(without zip method)\n",
        "list1=[1,2,3,4,5,7,8]\n",
        "list2=[\"a\", \"b\", \"c\", \"d\", \"e\"]\n",
        "dict1={}\n",
        "for key in list1:\n",
        "  for value in list2:\n",
        "    dict1[key]=value\n",
        "    list2.remove(value)\n",
        "    break\n",
        "print((dict1))"
      ],
      "execution_count": 2,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
            "{1: 'a', 2: 'b', 3: 'c', 4: 'd', 5: 'e'}\n"
          ],
